{
  "name": "vue-debug",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:css && vite",
    "dev:css": "npx tailwindcss -i ./src/components/debug/tailwind.css -o ./src/assets/debug.css",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "watch:css": "npx tailwindcss -i ./src/components/debug/tailwind.css -o ./src/assets/debug.css --watch"
  },
  "dependencies": {
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.0.0",
    "postcss-import": "^15.1.0",
    "prettier": "^2.8.4",
    "tailwindcss": "^3.2.6",
    "typescript": "^4.9.3",
    "vite": "^4.1.0",
    "vue-tsc": "^1.0.24"
  }
}
