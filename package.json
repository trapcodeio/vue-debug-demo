{
  "name": "vue-debug",
  "private": true,
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "dev": "npm run dev:css && vite",
    "dev:css": "npx tailwindcss -i ./src/components/debug/tailwind.css -o ./src/assets/debug.css",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "watch:css": "npx tailwindcss -i ./src/components/debug/tailwind.css -o ./src/assets/debug.css --watch"
  },
  "dependencies": {
    "vue": "^3.3.8",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^4.4.0",
    "@vueuse/core": "^10.5.0",
    "postcss-import": "^15.1.0",
    "prettier": "^3.0.3",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vite": "^4.5.0",
    "vue-tsc": "^1.8.22"
  }
}
